{
  "name": "nau-webpage",
  "version": "1.0.0",
  "description": "A redesigned page for the restaurant NAU Frutos do Mar using HTML, CSS, SASS and Bootstrap 4",
  "main": "index.js",
  "scripts": {
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/scss/ -o src/css",
    "build-task:autoprefixer": "postcss src/css/*.css --use autoprefixer -d src/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar \"src/scss/**/*.scss\" -c \"npm run sass:build\"",
    "live-server": "live-server",
    "dev": "npm-run-all -p sass:* live-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Restaurant",
    "HTML",
    "CSS",
    "SASS",
    "Bootstrap",
    "Redesign",
    "Paiva"
  ],
  "author": "Paiva",
  "license": "MIT",
  "devDependencies": {
    "gulp": "^4.0.0",
    "gulp-sass": "^4.0.2"
  },
  "dependencies": {
    "autoprefixer": "^9.7.2",
    "bootstrap": "^4.3.1",
    "chokidar-cli": "^2.1.0",
    "jquery": "^3.4.1",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "popper.js": "^1.14.7",
    "postcss-cli": "^6.1.3"
  }
}